{
  "name": "wncaab-perf-v3.1",
  "version": "3.1.0",
  "description": "WNCAAB Performance Visualization v3.1 - Grep-first metrics with schema validation",
  "scripts": {
    "rules:config": "bun run scripts/rules-config.js",
    "rules:pr": "bun run scripts/rules-pr.js",
    "rules:validate": "bun run scripts/rules-validate.js",
    "grep:perf": "rg --type md '\\[perf.*\\]' --colors match:fg:cyan:style:bold",
    "grep:proc-state": "rg '\\[PERF:.*proc\\]' --type md --files-with-matches",
    "grep:high-mem": "rg '\\[MEM:.*[50-9]\\d*\\.\\d*MB\\]' --type md --no-heading",
    "index:perf": "rg --files-with-matches --no-messages '\\[PERF:.*\\]' > .perf.index && echo '游릭 Perf Index built!'",
    "validate:perf": "bun run scripts/validate-perf.js",
    "grep:remote": "rg --type toml 'remote' --colors match:fg:cyan:style:bold",
    "grep:index": "rg 'index' --type toml --files-with-matches",
    "grep:fallback": "rg 'fallback' --type toml --no-heading",
    "index:remote": "rg --files-with-matches --no-messages 'remote' > .remote.index && echo '游릭 Remote Index built!'",
    "validate:remote": "bun run scripts/validate-remote.js",
    "setup:secrets": "bun run scripts/setup-secrets.js",
    "index:scan": "bun run scripts/index-generator.ts build",
    "grep:config": "rg --type toml '\\[\\w+\\]' --colors 'match:fg:cyan' --colors 'match:style:bold'",
    "grep:install": "rg '\\[install\\]' --type toml --files-with-matches",
    "grep:preload": "rg 'preload' --type toml --no-heading",
    "index:config": "rg --files-with-matches --no-messages '\\[\\w+\\]' > .config.index && echo '游릭 Config Index built!'",
    "validate:config": "bun run scripts/validate-config.js",
    "validate:bunfig": "bun run scripts/validate-bunfig.js",
    "grep:config-errors": "rg 'validation failed' --colors 'match:fg:red' --colors 'match:style:bold'",
    "index:config-errors": "rg --files-with-matches --no-messages 'validation failed' > .bunfig-errors.index && echo '游릭 Error Index built!'",
    "worker:telemetry": "bun run scripts/worker-telemetry-api.ts",
    "grep:immunity": "rg --type toml '\\[IMMUNITY:.*\\]' --colors 'match:fg:yellow' --colors 'match:style:bold'",
    "grep:hoisted": "rg '\\[IMMUNITY:hoisted\\]' --type toml --files-with-matches",
    "grep:auto-disable": "rg '\\[IMMUNITY:.*disable\\]' --type toml --no-heading",
    "grep:by-auto": "rg '\\[IMMUNITY:.*\\]' --type toml",
    "index:immunity": "rg --files-with-matches --no-messages '\\[IMMUNITY:.*\\]' > .immunity.index && echo '游릭 Immunity Index built!'",
    "validate:immunity": "bun run scripts/validate-immunity.js",
    "spline:api": "bun run scripts/spline-api.ts",
    "spline:render": "bun run scripts/spline-render.ts",
    "spline:predict": "bun run scripts/curve-predict.ts",
    "spline:bench": "bun run scripts/worker-bench.ts",
    "spline:deploy": "bun run scripts/preset-deploy.ts",
    "grep:ai-immunity": "rg --type toml '\\[AI-IMMUNITY:.*\\]' --colors 'match:fg:yellow' --colors 'match:style:bold'",
    "grep:ai-hoisted": "rg '\\[AI-IMMUNITY:ai-hoisted[^]]*\\]' --type toml --files-with-matches",
    "grep:ai-disable": "rg '\\[AI-IMMUNITY:[^]]*ai-disable[^]]*\\]' --type toml --no-heading",
    "index:ai-immunity": "bun run scripts/index-ai-immunity.ts",
    "map:edge": "bun run scripts/map-edge.ts",
    "absorb:cli-output": "bun run graph-propagation/cli-absorb.ts",
    "audit:ai-immunity": "bun run scripts/validate-ai-immunity.js",
    "gen:ai-tag": "bun run templates/ai-immunity-gen.js",
    "ai:config": "bun run scripts/ai-config.js",
    "ai:validate": "bun run scripts/validate-ai-immunity.js",
    "ai:pr": "bun run scripts/ai-pr.js",
    "bun:pr": "bun run scripts/bun-pr-test.ts",
    "bun:pr-asan": "bun run scripts/bun-pr-test.ts --asan",
    "bun:pr-version": "bun run scripts/bun-pr-test.ts --version",
    "atomic:pr": "bun run scripts/atomic-pr.js",
    "dev": "PORT=3002 bun --hot scripts/dev-server.ts",
    "dev:server": "bun run scripts/dev-server.ts",
    "prod": "bun scripts/dev-server.ts",
    "build:tension-states": "bun run build/tension-page-macro.ts",
    "build:tension-page": "bun run build/render-tension-page.ts",
    "build:tension": "bun run build:tension-states && bun run build:tension-page",
    "version": "bun run scripts/version-tracker.js",
    "version:check": "bun run scripts/version-tracker.js"
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "bin": {
    "atomic-pr": "./scripts/atomic-pr.js"
  },
  "bunReleaseNotes": "BUN-RELEASE-NOTES-ENHANCED.md",
  "changelog": "CHANGELOG.md"
}

